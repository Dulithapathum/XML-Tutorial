<!-- Root element representing a specific movie by its ID, with a default namespace -->
<movies id="5" xmlns="http://example.com/movies">
  
  <!-- The name of the movie -->
  <name>X-man</name>
  
  <!-- The director of the movie -->
  <director>Scott Cooper</director>
  
  <!-- The release year of the movie -->
  <release>2022</release>

</movies>
